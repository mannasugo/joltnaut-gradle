<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout 
    android:layout_width="match_parent" android:layout_height="match_parent" android:padding="16sp"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:layout_alignParentTop="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_centerVertical="true"
                android:layout_marginLeft="16sp"
                android:fontFamily="@font/v202201311917___litera___basic"
                android:textColor="#6C6C6C"
                android:textFontWeight="900" android:textSize="14sp"
                android:letterSpacing=".15"
                android:text="WALLET CONTACTS"/>

            <ImageView android:id="@+id/addContact"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_alignParentEnd="true" android:layout_centerVertical="true" android:src="@drawable/ic_max_202306301845"/>

        </RelativeLayout>

    </RelativeLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>